{
    "compilerOptions": {
      "target": "ES5",                          
      "lib": ["DOM", "ESNext"],                 
      "module": "ESNext",                       
      "moduleResolution": "Node",               
      "resolveJsonModule": true,                
      "strict": true,                           
      "esModuleInterop": true,                  
      "skipLibCheck": true,                     
      "forceConsistentCasingInFileNames": true, 
      "jsx": "react-jsx"                        
    },
    "include": ["src"],                        
    "exclude": ["node_modules", "build"]       
  }
  